{
  "name": "vue-cheatsheet",
  "displayName": "Vue Cheatsheet",
  "description": "Open Vue cheatsheet inside the editor",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:extension.openVuePdf",
  ],
  "main": "./out/extension.js",
  "license": "MIT",
  "icon": "asset/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "extension.openVuePdf",
        "title": "Vue Cheatsheet: Vue"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jojoee/vscode-vue-cheatsheet"
  },
  "publisher": "jojoee",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vsce": "^1.74.0",
    "vscode-test": "^1.3.0"
  }
}